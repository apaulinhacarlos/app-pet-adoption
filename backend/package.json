{
  "name": "pet-adoption-backend",
  "version": "1.0.0",
  "description": "backend do app de adocao pet",
  "main": "./build/server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "predev": "npm run db:reset",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "start": "ts-node ./src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "author": "apaulinhacarlos",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "4.19.2",
    "express-async-errors": "^3.1.1",
    "mysql2": "3.10.1",
    "sequelize": "6.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.5",
    "@types/sequelize": "4.28.20",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "nodemon": "3.1.3",
    "sequelize-cli": "6.6.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  }
}
